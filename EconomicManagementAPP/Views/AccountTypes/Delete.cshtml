@model AccountTypes

@{
    ViewData["Title"] = "Delete";
}

<h3 class="mb-3 titlesGen">¿Are you sure you want to delete the account type? @Model.Name? this type of account has @Model.NumberAccount?  associated accounts </h3>

<form asp-action="DeleteAccount">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger titlesGen">Delete</button>
    <a class="btn btn-secondary titlesGen" asp-controller="Home" asp-action="Index">Cancel</a>
</form>

@section Scripts {
<partial name="_ValidationScriptsPartial" />
}