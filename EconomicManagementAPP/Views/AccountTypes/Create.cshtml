@model AccountTypes
@{
    ViewData["Title"] = "Account types";
}

<h1 class="titles">Register account type</h1>
<div asp-validation-summary="ModelOnly" class="text-danger"></div>

<form asp-action="Create">
    <div class="mb-3 titlesGen">
        <label asp-for="Name" class="form-label"></label>
        <input type="text" asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"> </span>
    </div>
    <button type="submit" class="btn btn-primary titlesGen">Create</button>
    <a class="btn btn-secondary titlesGen" asp-controller="Home" asp-action="Index">Cancel</a>


</form>

@section Scripts {
<partial name="_ValidationScriptsPartial" />
}